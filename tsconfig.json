{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/Quotes.jsx",
    "app/(tabs)/Portfolio.jsx",
    "app/(tabs)/Trade.jsx",
    "app/(tabs)/History.jsx",
    "app/modal.jsx"
  ]
}
